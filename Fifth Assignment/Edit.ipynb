    "from Bio import SeqIO\n",
    "import editdistance\n",
    "\n",
    "def edit(a,b):\n",
    "    alignments = editdistance.eval(a,b)\n",
    "    print(alignments)\n",
    "\n",
    "\n",
    "data=list(SeqIO.parse(\"rosalind_edit.txt\",\"fasta\"))\n",
    "a=data[0].seq\n",
    "b=data[1].seq\n",
    "edit(a,b)"
   ]
