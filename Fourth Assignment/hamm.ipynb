    "def hamm (s,t):\n",
    "    diff=0\n",
    "    for i in range(len(s)):\n",
    "        if s[i]!=t[i]:\n",
    "            diff+=1\n",
    "    \n",
    "    return diff\n",
    "    \n",
    "data=open('rosalind_hamm.txt','r').readlines()\n",
    "s,t=data[0],data[1]\n",
    "print(hamm(s,t))"
   ]
