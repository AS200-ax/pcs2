    "def fibRabbit(n,k):\n",
    "    nMin2=1\n",
    "    nMin1=1\n",
    "    for i in range(n-2):\n",
    "        Rab=nMin2*k + nMin1\n",
    "        nMin2=nMin1\n",
    "        nMin1=Rab\n",
    "        \n",
    "    return Rab\n",
    "\n",
    "data=list(map(int, open('rosalind_fib.txt','r').read().split()))\n",
    "n,k=data[0],data[1]\n",
    "print(fibRabbit(n,k))"
   ]
