    "def median(A,k):\n",
    "    A=sorted(A)\n",
    "    return A[k-1]\n",
    "\n",
    "data = open('rosalind_med.txt','r')\n",
    "data = data.readlines()\n",
    "\n",
    "A = list(map(int,data[1].split()))\n",
    "k = int(data[2])\n",
    "\n",
    "print(median(A,k))"
   ]
