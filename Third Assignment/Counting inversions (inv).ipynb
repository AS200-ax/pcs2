   "source": [
    "def inv(A):\n",
    "    num=0\n",
    "    for i in range(len(A)-1):\n",
    "        if A[i]>=A[i+1]:\n",
    "            num+=1\n",
    "    return num\n",
    "\n",
    "data = open('rosalind_inv.txt', 'r')\n",
    "data = data.readlines()\n",
    "A = list(map(int, data[1].split()))\n",
    "print(inv(A))"
   ]
